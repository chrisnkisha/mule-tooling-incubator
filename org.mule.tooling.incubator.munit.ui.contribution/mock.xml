<?xml version="1.0" encoding="UTF-8" standalone="no"?> <!-- munit components definitions -->
<namespace xmlns="http://www.mulesoft.org/schema/mule/tooling.attributes"
	url="http://www.mulesoft.org/schema/mule/mock" prefix="mock">
	
	<global localId="config" 
		versions="[3.4.0,10.0.0]"
		caption="Mock configuration"
	 	category="org.mule.tooling.category.munit"
	 	allowMulipleChildren="true"
	 	icon="icons/small/MUnit-Palette.png" 
	 	image="icons/small/MUnit-Palette.png"
		description="Defines the Munit Mock condifiguration for the suite">
		
		<attribute-category description="General properties" caption="General">
			<group caption="Generic" id="name">
				<name required="true" description="Name of the mock configuration so it can be later referenced by config-ref." caption="Name" name="name"></name>
			</group>
		</attribute-category>
	</global>
	
	<nested localId="with-attributes" required="false">
	    <childElement visibleInDialog="false" allowMultiple="true" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/with-attribute"/>
	</nested>
	
	<nested localId="with-attribute" required="false">
		<string indented="true" name="whereValue-ref" caption="whereValue-ref" visibleInDialog="false"/>
		<string indented="true" name="name" caption="name" visibleInDialog="false"/>
	</nested>
	
	<nested localId="then-return" required="false">
		<string indented="true" name="payload-ref" caption="payload-ref" visibleInDialog="false"/>
	    <childElement visibleInDialog="false" allowMultiple="false" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/invocation-properties"/>
	    <childElement visibleInDialog="false" allowMultiple="false" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/inbound-properties"/>
	    <childElement visibleInDialog="false" allowMultiple="false" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/outbound-properties"/>
	</nested>
	
	<nested localId="invocation-properties" required="false">
	    <childElement visibleInDialog="false" allowMultiple="true" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/invocation-property"/>
	</nested>
	
	<nested localId="invocation-property" required="false">
		<string indented="true" name="key" caption="key" visibleInDialog="false"/>
		<string indented="true" name="value-ref" caption="value-ref" visibleInDialog="false"/>
	</nested>
	
	
	<nested localId="inbound-properties" required="false">
	    <childElement visibleInDialog="false" allowMultiple="true" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/inbound-property"/>
	</nested>
	
	<nested localId="inbound-property" required="false">
		<string indented="true" name="key" caption="key" visibleInDialog="false"/>
		<string indented="true" name="value-ref" caption="value-ref" visibleInDialog="false"/>
	</nested>
	
	<nested localId="outbound-properties" required="false">
	    <childElement visibleInDialog="false" allowMultiple="true" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/outbound-property"/>
	</nested>
	
	<nested localId="outbound-property" required="false">
		<string indented="true" name="key" caption="key" visibleInDialog="false"/>
		<string indented="true" name="value-ref" caption="value-ref" visibleInDialog="false"/>
	</nested>
	
	<component localId="when"
		versions="[3.4.0,10.0.0]"
		category="org.mule.tooling.category.munit"
		caption="Mock"  
		description="Mocking Definition" 
		image="icons/large/Mock.png" 
		icon="icons/small/Mock-Palette.png"
		acceptedByElements="http://www.mulesoft.org/schema/mule/munit/setup">
		
		<attribute-category description="Mocking conditions and return value." caption="Mock Condition">
			<group caption="General Settings" id="generalSettings">
				<globalRef name="config-ref" caption="Mock Configuration" required="true"
					description="The name of the global configuration to be used."
					requiredType="http://www.mulesoft.org/schema/mule/mock/config"/>
			</group>
			
      		<group caption="Message processor Mocking definition" id="when">
      			   <string indented="true" name="messageProcessor" caption="messageProcessor" visibleInDialog="false"/>     		
      			   <childElement visibleInDialog="false" allowMultiple="false" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/with-attributes"/>
      			   <childElement visibleInDialog="false" allowMultiple="false" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/then-return"/>
        		   <custom javaType="java.lang.String"
        	  		class="org.mule.tooling.ui.contribution.munit.editors.custom.MockCustomEditor"></custom>
        	  		
      		</group>
    	</attribute-category>
	</component>
	
	<component localId="throw-an"
		versions="[3.4.0,10.0.0]"
		category="org.mule.tooling.category.munit"
		caption="Throw An"  
		description="Mocking Definition that Throws an Exception" 
		image="icons/large/Mock.png" 
		icon="icons/small/Mock-Palette.png"
		acceptedByElements="http://www.mulesoft.org/schema/mule/munit/setup">
		
		<attribute-category description="Mocking conditions and throw an Exception." caption="Mock Condition">
			<group caption="General Settings" id="generalSettings">
				<globalRef name="config-ref" caption="Mock Configuration" required="true"
					description="The name of the global configuration to be used."
					requiredType="http://www.mulesoft.org/schema/mule/mock/config"/>
			</group>
			
      		<group caption="Message processor Mocking definition" id="throw-an">
      			   <string name="whenCalling" caption="messageProcessor" indented="true" visibleInDialog="false"/>     		
      			   <childElement visibleInDialog="false" allowMultiple="false" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/with-attributes"/>
        		   <custom javaType="java.lang.String" class="org.mule.tooling.ui.contribution.munit.editors.custom.MockThrowCustomEditor"></custom>
      		</group>
      		
      		<group caption="Exception To Be Thrown" id="exception-to-be-thrown">
				<string name="exception-ref" defaultValue="#[]" alwaysFill="true" description="An Then Throw Exception"  indented="true" inplace="true"  />
      		</group>
      		
    	</attribute-category>
	</component>
	
	 
	<component localId="verify-call"
		versions="[3.4.0,10.0.0]"
		category="org.mule.tooling.category.munit" 
		description="Verify calls definition" 
		image="icons/large/Verify-Call.png" 
		icon="icons/small/Verify-Call-Palette.png"		
		caption="Verify Call">

		
		<attribute-category description="Mocking conditions and return value." caption="Verify Condition">
			<group caption="General Settings" id="generalSettings">
				<globalRef name="config-ref" caption="Mock Configuration" required="true"
					description="The name of the global configuration to be used."
					requiredType="http://www.mulesoft.org/schema/mule/mock/config"/>
			</group>
			
      		<group caption="Message processor Verify definition" id="verify-call">
      			<string indented="true" name="messageProcessor" caption="messageProcessor" visibleInDialog="false"/>     		
      			   
      			<childElement visibleInDialog="false" allowMultiple="false" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/attributes"/>
				<custom javaType="java.lang.String" class="org.mule.tooling.ui.contribution.munit.editors.custom.VerifyCustomEditor"></custom>
      		</group>
      		
      		<group caption="Message processor Verify Times" collapsable="true" collapsed="true">
				<radioBoolean name="times-r" caption="Times"  fillLine="true" group="quantity" />
				<string name="times" description="Number of times this message processor should have been called"  indented="true" inplace="true"  controlled="times-r" />
				
				<radioBoolean name="at-least-r" caption="At Least" fillLine="true" group="quantity" />
				<string name="atLeast" description="Number of times this message processor should have been called"  indented="true" inplace="true" controlled="at-least-r" />
				
				<radioBoolean name="at-most-r" caption="At Most" fillLine="true" group="quantity" />
				<string name="atMost"  description="Number of times this message processor should have been called"  indented="true" inplace="true" controlled="at-most-r" />
      		</group>
      		
    	</attribute-category>
	</component>
	
	<nested localId="attributes" required="false">
	    <childElement visibleInDialog="false" allowMultiple="true" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/attribute"/>
	</nested>
	
	<nested localId="attribute" required="false">
		<string indented="true" name="whereValue-ref" caption="whereValue-ref" visibleInDialog="false"/>
		<string indented="true" name="name" caption="name" visibleInDialog="false"/>
	</nested>
	
	<component localId="outbound-endpoint"
		versions="[3.4.0,10.0.0]"
		category="org.mule.tooling.category.munit"  
		description="Mocking outbound endpoint by address"
		image="icons/large/Mock.png" 
		icon="icons/small/Mock-Palette.png"
		acceptedByElements="http://www.mulesoft.org/schema/mule/munit/setup"
		caption="Mock Endpoint">

		<keywords>
			<keyword value="mock" weight="1" />
			<keyword value="outbound-endpoint" weight="1" />
		</keywords>
		<attribute-category caption="General" description="General Endpoint Mock properties">
			<group caption="General Settings" id="generalSettings">
				<globalRef name="config-ref" caption="Mock Configuration" required="true"
					description="The name of the global configuration to be used."
					requiredType="http://www.mulesoft.org/schema/mule/mock/config"/>
			</group>
			
			<string caption="When Address" required="true" name="address" description="Endpoint address"/>
			<string caption="Then Return" required="false" name="returnPayload-ref" description="Return Payload"/>
		</attribute-category>
	</component>
	
	
	<container localId="spy"
		versions="[3.4.0,10.0.0]"
		category="org.mule.tooling.category.munit"  
		caption="Spy"
		description="Spy calls definition" 
		image="icons/large/Spy.png" 
		icon="icons/small/Spy-Palette.png"
		titleColor="255, 204, 102"
		acceptedByElements="http://www.mulesoft.org/schema/mule/munit/setup"
		pathExpression="${index}">
		
		<nested-container pathExpression="${index}" localId="assertions-before-call" caption="Assertions Before Call" description="" />
		<nested-container pathExpression="${index}" localId="assertions-after-call" caption="Assertions After Call" description=""/>
		
		<attribute-category description="Spy conditions" caption="Spy Condition">
      		<group caption="General Settings" id="generalSettings">
				<globalRef name="config-ref" caption="Mock Configuration" required="true"
					description="The name of the global configuration to be used."
					requiredType="http://www.mulesoft.org/schema/mule/mock/config"/>
			</group>
			
      		<group caption="Message processor Spy definition" id="spy">
      			   <string indented="true" name="messageProcessor" caption="messageProcessor" visibleInDialog="false"/>     		
      			   <childElement visibleInDialog="false" allowMultiple="false" inplace="true" required="false" description="" caption="" name="http://www.mulesoft.org/schema/mule/mock/with-attributes"/>
        		     <custom javaType="java.lang.String"
        	  		class="org.mule.tooling.ui.contribution.munit.editors.custom.SpyCustomEditor"></custom>
      		</group>
    	</attribute-category>
	</container>
	
	
</namespace>
